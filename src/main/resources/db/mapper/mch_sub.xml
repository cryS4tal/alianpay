<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ylli.api.mch.mapper.MchSubMapper">
    <resultMap id="subMap" type="com.ylli.api.mch.model.MchSub"/>

    <select id="agencyList" resultMap="subMap">
        SELECT * FROM t_mch_sub
        <where>
            <if test="type != null">
                type = ${type}
            </if>
            <if test="mch_id != null">
                AND mch_id = ${mch_id}
            </if>
            <if test="sub_id != null">
                AND sub_id = ${sub_id}
            </if>
        </where>
    </select>
</mapper>