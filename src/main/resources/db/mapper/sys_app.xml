<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ylli.api.mch.mapper.SysAppMapper">
    <resultMap id="sysAppMap" type="com.ylli.api.mch.model.SysApp"/>

    <select id="getSysApp" resultMap="sysAppMap">
        SELECT
            *
        FROM t_sys_app
        <where>
            <if test="app_name != null">
                app_name = #{app_name}
            </if>
            <if test="status != null">
                AND status = #{status}
            </if>
        </where>
    </select>
</mapper>